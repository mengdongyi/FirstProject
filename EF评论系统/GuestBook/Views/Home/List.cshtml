@model IEnumerable<EF.Message>
@{
    ViewBag.Title = "List";
}


<table>
    <tr>
      <th>Id</th><th>标题</th><th>内容</th><th>发布时间</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.id
            </td>
            <td>@item.MessageTitle
            </td>
            <td>@item.MessageContent
            </td>
            <td>@item.PostTime
            </td>
        </tr>
    }
</table>

